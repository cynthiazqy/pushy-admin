{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 10"],
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@ant-design/pro-layout": "^5.0.9",
    "@ant-design/pro-table": "2.2.2",
    "antd": "^4.1.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.5",
    "umi": "3.1.1",
    "umi-request": "^1.2.19",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.20",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@umijs/fabric": "^2.0.8",
    "@umijs/plugin-blocks": "^2.1.3",
    "@umijs/preset-ant-design-pro": "^1.2.1",
    "@umijs/preset-react": "^1.4.16",
    "@umijs/preset-ui": "^2.1.12",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.2.1",
    "detect-installer": "^1.0.2",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "jsdom-global": "^3.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.4",
    "pro-download": "1.0.1",
    "puppeteer-core": "^3.0.0"
  },
  "engines": { "node": ">=10.0.0" },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
